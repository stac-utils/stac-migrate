{
  "stac_version":"1.0.0",
  "stac_extensions": [
    "https://stac-extensions.github.io/eo/v1.0.0/schema.json",
    "https://stac-extensions.github.io/raster/v1.0.0/schema.json",
    "https://stac-extensions.github.io/projection/v1.1.0/schema.json"
  ],
  "id":"example",
  "title":"Example",
  "description":"Example",
  "license":"Apache-2.0",
  "extent":{
    "spatial":{
      "bbox":[
        [-50,-90,50,90]
      ]
    },
    "temporal":{
      "interval":[
        ["1990-01-01T00:00:00Z","2000-01-01T00:00:00Z"]
      ]
    }
  },
  "links":[],
  "summaries":{
    "eo:bands":[
      {
        "name":"B1",
        "common_name":"coastal",
        "center_wavelength":4.439,
        "gsd":60
      },
      {
        "name":"B2",
        "common_name":"blue",
        "center_wavelength":4.966,
        "gsd":10
      }
    ]
  },
  "assets": {
    "example": {
      "href": "https://example.com/image.tif",
      "type": "image/tiff",
      "proj:epsg": 4326,
      "eo:bands":[
        {
          "name":"B1",
          "common_name":"coastal",
          "center_wavelength":4.439,
          "gsd":60
        },
        {
          "name":"B2",
          "common_name":"blue",
          "center_wavelength":4.966,
          "gsd":10
        }
      ],
      "raster:bands":[
        {
          "name":"B1",
          "data_type":"uint16",
          "sampling":"area",
          "spatial_resolution":60
        },
        {
          "name":"B2",
          "data_type":"uint16",
          "sampling":"area",
          "spatial_resolution":10
        },
        {
          "name":"SCL",
          "data_type":"uint8",
          "spatial_resolution":60
        }
      ]
    }
